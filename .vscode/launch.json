    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${fileBasename}",
            "args": [
                "source:testData/source",
                "dest:testData/dest",
                "-v",
                "-c",
                "out:results-code.json",
                "ext:JPG", "ext:jpg", "ext:png", "ext:bmp", "ext:mp4",
                "bash:name echo line:$line size:$size source:$source key:$key",
                "bashfile:bash-code.sh"
            ]
        }
    ]
}